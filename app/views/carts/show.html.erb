
<p id="notice"><%= notice %></p>

<h1>Carts</h1>

<table class="table">
  <thead>
  <tr>
    <th scope="col">Name</th>
    <th scope="col">Description</th>
    <th scope="col">Price</th>
    <th scope="col">Account_id</th>
  </tr>
  </thead>

  <tbody>
  <% @products.each do |product| %>

    <tr>
      <td><%= product.name %></td>
      <td><%= product.description %></td>
      <td><%= product.price %></td>
      <td><%= product.cart_id %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

